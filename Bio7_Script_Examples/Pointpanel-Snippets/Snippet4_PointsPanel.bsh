/* This Snippet demonstrates how to get the ellipses in the 
Points panel. The Java API offers some specialized methods
for the class Ellipse2D!*/

import java.awt.geom.Ellipse2D;
import java.awt.geom.Ellipse2D.Double;
Ellipse2D.Double[] ellipse = PointPanel.getEllipse();

for (int i = 0; i < ellipse.length; i++) {

	int diamter_points = ellipse[i].getHeight();
	System.out.println("diameter: " + diamter_points);

}